{
  "name": "extra-response",
  "version": "0.3.6",
  "description": "Utilities for Response",
  "keywords": [],
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/es2018/index.js",
  "types": "lib/es2018/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-response.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "karma start karma.config.js --single-run",
    "prepublishOnly": "run-s clean build bundle",
    "clean": "run-p clean:*",
    "clean:build": "rimraf lib",
    "clean:bundle": "rimraf dist",
    "build": "run-p build:*",
    "build:es2015": "run-s build:es2015:*",
    "build:es2015:compile": "tsc --project tsconfig.build.json --module commonjs --target es2015 --outDir lib/es2015",
    "build:es2015:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2015",
    "build:es2018": "run-s build:es2018:*",
    "build:es2018:compile": "tsc --project tsconfig.build.json --module commonjs --target es2018 --outDir lib/es2018",
    "build:es2018:patch": "tscpaths -p tsconfig.build.json -s ./src -o ./lib/es2018",
    "bundle": "rollup --config rollup.config.js",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/http-status": "^1.0.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jasmine": "^3.9.1",
    "@types/papaparse": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "husky": "4",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-webpack": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "return-style": "^0.12.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.3.1",
    "ts-loader": "^9.2.5",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tscpaths": "^0.0.9",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1"
  },
  "dependencies": {
    "iterable-operator": "^0.14.3",
    "papaparse": "^5.3.1"
  },
  "peerDependencies": {
    "@blackglory/http-status": "^1.0.0"
  }
}
